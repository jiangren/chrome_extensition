{
    "manifest_version": 2,
    "name": "copy_page_url",
    "description": "first Chrome Extensition",
    "version": "0.0.1",

    "browser_action": {
        "default_icon": {
            "19": "images/icon19.png",
            "18": "images/icon18.png"
        },
        "default_title": "copy url",
        "default_popup": "popup.html"
    },

    "options_page": "options.html",

    "permissions": [
        "tabs",
        "activeTab",
        "storage"
    ]
}